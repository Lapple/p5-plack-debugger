{
  "name": "p5-plack-debugger",
  "version": "1.0.0",
  "description": "A new debugging tool for Plack web applications",
  "scripts": {
    "js": "browserify share/js/source/main.js -o share/js/index.js",
    "watch": "watchify share/js/source/main.js -o share/js/index.js -v",
    "example": "starman --host 127.0.0.1 --port 8080 -I lib example/app.psgi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevan/p5-plack-debugger.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevan/p5-plack-debugger/issues"
  },
  "homepage": "https://github.com/stevan/p5-plack-debugger",
  "devDependencies": {
    "browserify": "^6.0.3",
    "es5-shim": "^4.0.3",
    "keymaster": "^1.6.2",
    "react": "^0.11.2",
    "reactify": "^0.14.0",
    "underscore": "^1.7.0",
    "watchify": "^2.0.0"
  },
  "browser": {
    "react": "./share/js/source/react.js"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
