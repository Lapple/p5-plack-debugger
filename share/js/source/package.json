  "bugs": {
    "url": "https://github.com/stevan/p5-plack-debugger/issues"
  },
  "homepage": "https://github.com/stevan/p5-plack-debugger",
  "devDependencies": {
    "browserify": "^6.0.3",
    "es5-shim": "^4.0.3",
    "keymaster": "^1.6.2",
    "react": "^0.11.2",
    "react-json-inspector": "^1.0.2",
    "reactify": "^0.14.0",
    "underscore": "^1.7.0",
    "watchify": "^2.0.0"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
