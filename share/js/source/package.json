    "js": "NODE_ENV=production browserify main.js -o ../index.js",
    "watch": "watchify main.js -o ../index.js -v"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevan/p5-plack-debugger.git"
  },
